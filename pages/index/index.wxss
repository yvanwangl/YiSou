.container {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  padding: 0;
  background: #DEDEDE;
  height: auto;
}
.itemWrap {
  width: 100%;
  background: #fff;
  margin: 0 0 20rpx 0;
}
.itemWrap.last {
  margin: 0;
  border-bottom: 1rpx solid #eee;
}
.itemTitle {
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  border-bottom: 1rpx solid #E0E0E0;
}
.programItem {
  display:inline-block;
  box-sizing: border-box;
  width:25%;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1rpx solid #eee;
  border-right: 1rpx solid #eee;
  font-size: 28rpx;
}